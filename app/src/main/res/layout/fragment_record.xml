                android:textSize="15sp"
                android:background="@drawable/quick_rank_seletor"/>

            <Button
                android:id="@+id/btnRankRecord"
                android:layout_width="174dp"
                android:layout_height="40.3dp"
                android:text="경쟁전"
                android:textColor="#9b9b9b"
                android:textSize="15sp"
                android:background="@drawable/quick_rank_seletor"/>
