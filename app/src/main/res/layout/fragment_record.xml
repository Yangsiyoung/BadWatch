                        android:layout_marginLeft="7dp"
                        android:gravity="center_vertical|left"
                        android:textColor="#424242"
                        android:textSize="13.3sp"/>

                    <TextView
                        android:id="@+id/txtUserMostMissionTime"
                        android:layout_width="86.7dp"
                        android:layout_height="36.7dp"
                        android:text="123"
                        android:layout_marginLeft="80dp"
                        android:gravity="center_vertical|right"/>

                </LinearLayout>

                <LinearLayout
                    android:orientation="horizontal"
                    android:gravity="center_vertical"
                    android:layout_width="match_parent"
                    android:layout_height="36.7dp">

                    <ImageView
                        android:layout_width="4.7dp"
                        android:layout_height="5.2dp"
                        android:background="@drawable/card"/>

                    <TextView
                        android:id="@+id/txtMostOneCombo"
                        android:layout_width="111dp"
                        android:layout_height="36.7dp"
                        android:text="원콤보 최고기록"
                        android:layout_marginLeft="7dp"
                        android:gravity="center_vertical|left"
                        android:textColor="#424242"
                        android:textSize="13.3sp"/>

                    <TextView
                        android:id="@+id/txtUserMostOneCombo"
