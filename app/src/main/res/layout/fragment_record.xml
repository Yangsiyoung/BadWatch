            android:layout_width="match_parent"
            android:layout_height="41dp"
            android:gravity="center_horizontal">

            <Button
                android:id="@+id/btnQuickRecord"
                android:layout_width="174dp"
                android:layout_height="40.3dp"
                android:text="빠른대전"
